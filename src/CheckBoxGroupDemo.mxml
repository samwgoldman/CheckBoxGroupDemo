<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx"
			   xmlns:examples="com.samgoldmansoftware.examples.*" viewSourceURL="srcview/index.html">
	<s:VGroup gap="0" paddingTop="10" paddingRight="10" paddingBottom="10" paddingLeft="10">
		<s:TabBar dataProvider="{examples}" />
		<mx:ViewStack id="examples" width="100%" height="100%" borderStyle="solid">
			<s:NavigatorContent label="selectedItems data binding">
				<examples:SelectedItemsBindingExample width="100%" height="100%" />
			</s:NavigatorContent>
			<s:NavigatorContent label="virtual layout">
				<examples:VirtualizedLayoutExample width="100%" height="100%" />
			</s:NavigatorContent>
			<s:NavigatorContent label="filtering data">
				<examples:DataFilteringExample width="100%" height="100%" />
			</s:NavigatorContent>
		</mx:ViewStack>
	</s:VGroup>
</s:Application>
